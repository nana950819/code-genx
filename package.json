{
	"name": "codegenx",
	"publisher": "DeepGenX",
	"displayName": "CodeGenX",
	"description": "CodeGenX - A code autocompletion tool based on GPT-J",
	"repository": {
		"type": "git",
		"url": "https://github.com/DeepGenX/CodeGenX"
	},
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:codegenx.open_CodeGenX"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"title": "CodeGenX: Get AI Completion",
				"command": "codegenx.open_CodeGenX"
			}
		],
		"configuration":[
			{
				"title": "CodeGenX",
				"properties": {
					"Codegenx.Temperature": {
						"type": "number",
						"default": 1.0,
						"description": "The temperature parameter of the GPT-J model"
					  },
					  "Codegenx.Top_P": {
						"type": "number",
						"default": 0.8,
						"description": "The top_p parameter of the GPT-J model"
					  },
					  "Codegenx.Top_K": {
						"type": "number",
						"default": 40,
						"description": "The top_k parameter of the GPT-J model"
					  },
					  "Codegenx.MaxLength": {
						"type": "string",
						"enum": [
                        "128 (fast)",
                        "264 (medium)",
						"512 (slow)"
                    ],
						"default": "128 (fast)",
						"description": "The Max length of the output. The longer the output the slower the inference."
					  },

					  "Codegenx.EnableSelection": {
						"type": "boolean",
						"default": true,
						"description": "Option to generate output for text which is selected."
					  },

					  "Codegenx.StopSequence": {
						"type": "string",
						"default": "<|endoftext|>",
						"description": "The stop_sequence parameter of the GPT-J model"
					  }
					  
				}
			}
		],
		"keybindings": [
            {
                "command": "codegenx.open_CodeGenX",
                "key": "ctrl+c",
                "mac": "cmd+c"
            }
        ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"axios": "^0.21.4"
	}
}
